      <embed src="./images/main_flash1.swf" quality="high" type="application/x-shockwave-flash" width="40%" height="99%">
            </embed>
      <div class="nav-box">
        <ul>
          <li class="cur"><a>首页</a></li>
          <li>
            <a href="javascript:void(0)" onclick="navFracas()">FRACAS管理</a>
            <input id="fracas_alert_icon" class="alert_icon" value="0" readonly="readonly" display="none"/>
          </li>
          <li>
            <a>合同管理</a>
            <input class="alert_icon" value="0" readonly="readonly"/>
          </li>
          <li>
            <a>部门运行数据管理</a>
            <input class="alert_icon" value="0" readonly="readonly"/>
          </li>
