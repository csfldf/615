<!DOCTYPE html>
<html>
<head>
    <title>camera组件</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <script src="../avalon.js"></script>
    <script src="../highlight/shCore.js"></script>
    <script>
        require(["camera/avalon.camera"], function () {
            avalon.define("demo1", function (vm) {
                vm.camera = {
                    pictures: [
                        "http://placehold.it/600x200/903/fff.jpg&text=img1",
                        "http://placehold.it/600x200/f36/fff.jpg&text=img2",
                        "http://placehold.it/600x200/669/fff.jpg&text=img3",
                        "http://placehold.it/600x200/0cf/fff.jpg&text=img4",
                        "http://placehold.it/600x200/09f/fff.jpg&text=img5",
                    ],
                    slicedCols: 10,
                    slicedRows: 3
                }
                vm.$skipArray = ["camera"]
            })
            avalon.define("demo2", function (vm) {
                vm.camera = {
                    pictures: [
                        "http://placehold.it/600x200/903/fff.jpg&text=img1",
                        "http://placehold.it/600x200/f36/fff.jpg&text=img2",
                        "http://placehold.it/600x200/669/fff.jpg&text=img3",
                        "http://placehold.it/600x200/0cf/fff.jpg&text=img4",
                        "http://placehold.it/600x200/09f/fff.jpg&text=img5",
                    ],
                    slicedCols: 5,
                    slicedRows: 10
                }
                vm.$skipArray = ["camera"]
            })
            avalon.define("demo3", function (vm) {
                vm.camera = {
                    pictures: [
                        "http://placehold.it/600x200/903/fff.jpg&text=img1",
                        "http://placehold.it/600x200/f36/fff.jpg&text=img2",
                        "http://placehold.it/600x200/669/fff.jpg&text=img3",
                        "http://placehold.it/600x200/0cf/fff.jpg&text=img4",
                        "http://placehold.it/600x200/09f/fff.jpg&text=img5",
                    ],
                    slicedCols: 3,
                    slicedRows: 2
                }
                vm.$skipArray = ["camera"]
            })

            avalon.scan()
        })
    </script>
    <style type="text/css">
        .widget {
            width: 600px;
            height: 200px;
            margin: 0 auto;
        }
    </style>
</head>
<body>
    <div class="wrapper">
        <h1>图片百叶窗切换组件-自定义区块数量</h1>
        <fieldset ms-controller="demo1">
            <legend>slicedCols: 10,slicedRows: 3</legend>
            <div class="widget" ms-widget="camera"></div>
        </fieldset>
        <fieldset ms-controller="demo2">
            <legend>slicedCols: 5,slicedRows: 10</legend>
            <div class="widget" ms-widget="camera"></div>
        </fieldset>
        <fieldset ms-controller="demo3">
            <legend>slicedCols: 3,slicedRows: 2</legend>
            <div class="widget" ms-widget="camera"></div>
        </fieldset>
        <pre ms-skip class="brush:html;gutter:false;toolbar:false">
