<hibernate-mapping package="org.sjtu.p615.dept_mgmt.entity">
	<class name="Cdnfile" table="cdnfile">
		<id 
			name="partName" 
			column="partName"
			type="string"
			length="50"
		></id>
	
	<!--  
		<property
			column="partName"
			length="50"
			name="partName"
			not-null="true"
			type="string"
		 />
		 -->
		<property
			column="cdnNode"
			length="255"
			name="cdnNode"
			not-null="false"
			type="string"
		 />
	</class>
</hibernate-mapping>