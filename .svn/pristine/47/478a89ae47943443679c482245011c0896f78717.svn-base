<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="org.sjtu.p615.entity">
	<class name="FailureReport" table="failurereport">
		<id
			column="FR_ID"
			name="id"
			type="integer"
		>
			<generator class="native" />
		</id>
		<property
			column="FailureInfo_ID"
			name="failureInfoId"
			type="integer"
		 />
		<property
			column="Environment"
			length="1024"
			name="environment"
			not-null="true"
			type="string"
		 />
		<property
			column="Description"
			length="1024"
			name="description"
			not-null="true"
			type="string"
		 />
		<property
			column="CommtiteeSign"
			length="128"
			name="commtiteeSign"
			not-null="true"
			type="string"
		 />
		<property
			column="Type"
			length="256"
			name="type"
			not-null="true"
			type="string"
		 />
		<property
			column="OperatorSign"
			length="128"
			name="operatorSign"
			not-null="true"
			type="string"
		 />
		<property
			column="TechnicalLeaderOpinion"
			length="1024"
			name="technicalLeaderOpinion"
			not-null="true"
			type="string"
		 />
		<property
			column="MRB"
			length="1024"
			name="mrb"
			not-null="true"
			type="string"
		 />
		<property
			column="SystemType"
			length="256"
			name="systemType"
			not-null="true"
			type="string"
		 />
		<property
			column="QualityIssueCategory"
			length="256"
			name="qualityIssueCategory"
			not-null="true"
			type="string"
		 />
		<property
			column="Mark"
			length="10"
			name="mark"
			not-null="false"
			type="integer"
		 />
		<property
			column="Time"
			length="10"
			name="time"
			not-null="true"
			type="date"
		 />
		<property
			column="FailureMode"
			length="1024"
			name="failureMode"
			not-null="true"
			type="string"
		 />
		<property
			column="OperatorSignDate"
			length="10"
			name="operatorSignDate"
			not-null="true"
			type="date"
		 />
		<property
			column="Process"
			length="1024"
			name="process"
			not-null="true"
			type="string"
		 />
		<property
			column="CommitteeSignDate"
			length="10"
			name="committeeSignDate"
			not-null="true"
			type="date"
		 />
		<property
			column="Reporter"
			length="256"
			name="reporter"
			not-null="true"
			type="string"
		 />
		<property
			column="Column_19"
			length="10"
			name="column19"
			not-null="false"
			type="string"
		 />
  		<!--many-to-one标签的含义,是在本表中增加外键指向另一端. 
  			name="failureBasicInfo"这个值必须与FailureReport类中的变量名一致   
      		unique="true"含义,是本表的本字段加上唯一性约束. 
    	--> 
		<many-to-one
			class="FailureBasicinfo"
			name="failureBasicInfo"
			unique="true"
			not-null="true"
			cascade="all"
		>
			<column name="FailureInfo_ID" />
		</many-to-one>
		
	</class>
</hibernate-mapping>
